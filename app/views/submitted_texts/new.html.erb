<h1>Texto a ser traduzido</h1>

<%= simple_form_for @submitted do |f| %>
  <%= f.input :url, label: 'Endereço no gov.br'%>
  <%= f.input :institution, label: 'Instituição:'%>
  <%= f.input :service_title, label: 'Nome do serviço:'%>
  <%= f.input :service, label: 'Serviço:'%>
  <%= f.input :target_public, label: 'Público alvo:'%>
  <%= f.input :service_stages, label: 'Etapas do serviço:' %>
  <%= f.input :more_info, label: 'Mais informações:' %>
  <%= f.input :deadline, label: 'Prazo:', as: :date, html5: true%>
  <%= f.submit "Traduzir", :class => "photobtn main_button" %>
<% end %>
